<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yaroslav.list_products_final">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.List_Products">
        <activity android:name="com.yaroslav.list_products_final.ui.InfoSelectProductActivity"></activity>
        <activity android:name="com.yaroslav.list_products_final.ui.CreateProductActivity" />
        <activity android:name="com.yaroslav.list_products_final.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>